{
  "name": "ngmeta",
  "version": "1.2.1",
  "description": "A tool for updating meta tags in an Angular application.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngmeta.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngmeta.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "documentation": "yuidoc . -c yuidoc.json",
    "start": "npm build",
    "postinstall": "node postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelSolati/ngmeta.git"
  },
  "keywords": [
    "angular",
    "angular 2",
    "angular2",
    "ng",
    "ng2",
    "meta",
    "tags",
    "seo"
  ],
  "author": "Michael Solati",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichaelSolati/ngmeta/issues"
  },
  "homepage": "https://ngmeta.michaelsolati.com/",
  "readme": "README.md",
  "peerDependencies": {
    "@angular/core": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.2.5",
    "@angular/compiler-cli": "^4.2.5",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "chalk": "^1.1.3",
    "rollup": "^0.43.0",
    "rxjs": "^5.1.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.23",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "@angular/router": "^4.0.0"
  }
}
